{
  "name": "car-recsys-system",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:frontend": "cd frontend && npm run dev",
    "dev:market": "cd car-market-recsys && npm run dev",
    "dev:all": "npm-run-all --parallel dev:frontend dev:market",
    "build:frontend": "cd frontend && npm run build",
    "build:market": "cd car-market-recsys && npm run build",
    "install:all": "cd frontend && npm install && cd ../car-market-recsys && npm install"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}

